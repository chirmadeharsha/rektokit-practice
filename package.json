{
  "name": "hea-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@reduxjs/toolkit": "1.9.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "8.1.2",
    "react-router-dom": "6.15.0",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently 'yarn start' 'yarn run prettier-watch' 'yarn run lint-watch'",
    "lint": "yarn prettier && eslint src/**/*.{ts,tsx} --fix",
    "lint-watch": "onchange 'src/**/*.{ts,tsx}' -- eslint {{changed}} --fix",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "prettier-watch": "onchange 'src/**/*.{ts,tsx}' -- prettier --write {{changed}}",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "12.8.3",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "concurrently": "8.0.1",
    "eslint": "8.47.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-pii": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-plugin-xss": "0.1.12",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "nodemon": "3.0.1",
    "onchange": "7.1.0",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}